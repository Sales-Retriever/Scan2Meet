# Google Gemini API Key (Required)
# Get your API key from Google AI Studio: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Query Parameter Names for Scheduling Links (Optional)
# Define the names for query parameters passed to the scheduling links. Defaults are used if not specified.
VITE_QUERY_PARAM_LAST_NAME=lastName
VITE_QUERY_PARAM_FIRST_NAME=firstName
VITE_QUERY_PARAM_FULL_NAME=fullName
VITE_QUERY_PARAM_DEPARTMENT=department
VITE_QUERY_PARAM_COMPANY=company
VITE_QUERY_PARAM_EMAIL=email
VITE_QUERY_PARAM_PHONE=phone

# PostHog (Optional)
VITE_PUBLIC_POSTHOG_KEY=YOUR_POSTHOG_KEY
VITE_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com
